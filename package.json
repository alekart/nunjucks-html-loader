{
  "name": "@alekart/nunjucks-html-loader",
  "version": "1.0.0",
  "description": "Nunjucks to static HTML loader for Webpack",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alekart/nunjucks-html-loader.git",
    "directory": "@alekart/nunjucks-html-loader"
  },
  "author": "alekart",
  "homepage": "https://github.com/alekart/nunjucks-html-loader",
  "bugs": "https://github.com/alekart/nunjucks-html-loader/issues",
  "main": "index.js",
  "engines": {
    "node": ">= 16.19.0"
  },
  "scripts": {
    "start": "npm run build -- -w",
    "clean": "del-cli dist",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production babel src -d dist --copy-files",
    "commitlint": "commitlint --from=main",
    "security": "npm audit",
    "lint:prettier": "prettier \"{**/*,*}.{js,json,md,yml,css,ts}\" --list-different",
    "lint:js": "eslint --cache .",
    "lint": "npm-run-all -l -p \"lint:**\"",
    "test:only": "NODE_ENV=test jest",
    "test:watch": "npm run test:only -- --watch",
    "test:coverage": "npm run test:only -- --collectCoverageFrom=\"src/**/*.js\" --coverage",
    "pretest": "npm run lint",
    "test": "npm run test:coverage",
    "copy-package-info": "node ./scripts/package-for-release.js",
    "prepare": "npm run build",
    "release": "standard-version"
  },
  "files": [
    "dist/",
    "lib/",
    "index.js"
  ],
  "peerDependencies": {
    "html-loader": "^4.2.0",
    "nunjucks": "^3.2.3",
    "webpack": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "glob": "^8.1.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@webpack-contrib/eslint-config-webpack": "^3.0.0",
    "babel-jest": "^29.4.2",
    "commitlint-azure-pipelines-cli": "^1.0.5",
    "cross-env": "^7.0.3",
    "del": "^7.0.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "html-loader": "^4.2.0",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "jest-junit": "^15.0.0",
    "lint-staged": "^13.1.1",
    "memfs": "^3.4.13",
    "nunjucks": "^3.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "standard-version": "^9.5.0",
    "webpack": "^5.75.0"
  },
  "keywords": [
    "webpack",
    "loader",
    "nunjcuks",
    "template",
    "html-webpack-plugin",
    "html-loader"
  ]
}
